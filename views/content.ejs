doctype html
html
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width-device-width, initial-scale=1.0')
    link(rel='stylesheet', href='/bootstrap/css/bootstrap.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    h2  
      a(href='/board') Welcome, board page!
    div
      table(class="table table-striped")
          tr
            th Number
            td=board.number
          tr
            th Title
            td=board.title
          tr
            th Author
            td=board.author
          tr
            th Date
            td=board.date
          tr
            th Hit
            td=board.hit
          tr
            th Content
            td
              pre=board.content
          tr
            td
              a(href='/board/edit/'+board.number) edit
            td
              a(href='/board/delete/'+board.number) delete
      
      
      
      
